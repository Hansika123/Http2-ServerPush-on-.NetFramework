    @using Test_Http2.Controllers

    @{
        ViewBag.Title = "Home Page";
    }

    @Html.PushPromiseStylesheet("~/Content/Site.css")
    @Html.PushPromiseStylesheet("~/content/bootstrap.css")

    <div class="jumbotron">
        <h1>ASP.NET Http2 Server Push</h1>
        <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    </div>

    <div class="row">
        <h1>Hello HTTP/2 World!</h1>
        @Html.PushPromiseImage("~/images/16.jpg")
        <hr />
        <p>
            HTTP/2 is a major revision of the HTTP network protocol used by the World Wide Web.
            It was derived from the earlier experimental SPDY protocol, originally developed by Google. <br>
            HTTP/2 was developed by the Hypertext Transfer Protocol working group httpbis of the Internet Engineering Task Force
        </p>

        <p><button type="button">Check Network Tab!</button></p>
        @Html.PushPromiseImage("~/images/11.jpg")
        @Html.PushPromiseImage("~/images/12.jpg")
        @Html.PushPromiseImage("~/images/13.jpg")
        @Html.PushPromiseImage("~/images/14.jpg")
        @Html.PushPromiseImage("~/images/15.jpg")

    </div>